<TrainFitLogNodeBody>
    width: dp(200)
    height: dp(335)
    InputField:
        height: dp(10)
        size_hint: 1, None
        id: data_input
        label_text: 'data input'
    InputField:
        height: dp(20)
        size_hint: 1, None
        id: target_input
        label_text: 'target input'
    GridLayout:
        height: dp(25)
        size_hint: 1, None
        cols: 3
        Label:
            height: dp(25)
            size_hint: 1, None
            text: 'test size  '
            halign: "right"
            valign: 'middle'
            text_size: self.size
        TextInput:
            height: dp(28)
            size_hint: 0.8, None
            foreground_color: 255,255,255,1
            background_normal: ''
            background_color: 0, 0, 0, 1
            id: test_size
            on_text: root.test_size = args[1]
            text: root.test_size
        Label:
            height: dp(25)
            size_hint: 0.2, None
            text: ' %'
            halign: "right"
            valign: 'middle'
            text_size: self.size
    GridLayout:
        height: dp(25)
        size_hint: 1, None
        cols: 3
        Label:
            height: dp(25)
            size_hint: 1, None
            text: 'train size  '
            halign: "right"
            valign: 'middle'
            text_size: self.size
        TextInput:
            height: dp(28)
            size_hint: 0.8, None
            foreground_color: 255,255,255,1
            background_normal: ''
            background_color: 0, 0, 0, 1
            id: train_size
            on_text: root.train_size = args[1]
            text: root.train_size
        Label:
            height: dp(25)
            size_hint: 0.2, None
            text: ' %'
            halign: "right"
            valign: 'middle'
            text_size: self.size
    GridLayout:
        height: dp(25)
        size_hint: 1, None
        cols: 2
        Label:
            height: dp(25)
            size_hint: 1, None
            italic: True
            text: 'random int  '
            halign: "right"
            valign: 'middle'
            text_size: self.size
        TextInput:
            height: dp(28)
            size_hint: 1, None
            foreground_color: 255,255,255,1
            background_normal: ''
            background_color: 0, 0, 0, 1
            id: random
            on_text: root.random = args[1]
            text: root.random
    LabeledCheckBox:
        label_text: 'shuffle'
        is_checked: root.shuffle
        on_is_checked: root.shuffle = args[1]
    Label:
        height: dp(20)
        size_hint: 1, None
        italic: True
        text: '      plot values'
    GridLayout:
        height: '25dp'
        size_hint: 1, None
        cols: 2
        Label:
            height: dp(25)
            size_hint: 0.15, None
            text: 'x  '
            halign: "right"
            valign: 'middle'
            text_size: self.size
        MultiSelection:
            height: dp(25)
            size_hint: 1, None
            items: [['CSV_1 Input', 'CSV_1 input'], ['CSV_2 Input', 'CSV_2 input'], ['CSV_1_Train Values', 'CSV_1 train'], ['CSV_2_Train Values', 'CSV_2 train'], ['CSV_1_Test Values', 'CSV_1 test'], ['CSV_2_Test Values', 'CSV_2 test']]
            selected_item: root.X_Plot_value
            on_selected_item: root.X_Plot_value = args[1]
    GridLayout:
        height: dp(25)
        size_hint: 1, None
        cols: 2
        Label:
            height: dp(25)
            size_hint: 0.15, None
            text: 'y  '
            halign: "right"
            valign: 'middle'
            text_size: self.size
        MultiSelection:
            height: dp(25)
            size_hint: 1, None
            items: [['CSV_1 Input', 'CSV_1 input'], ['CSV_2 Input', 'CSV_2 input'], ['CSV_1_Train Values', 'CSV_1 train'], ['CSV_2_Train Values', 'CSV_2 train'], ['CSV_1_Test Values', 'CSV_1 test'], ['CSV_2_Test Values', 'CSV_2 test']]
            selected_item: root.Y_Plot_value
            on_selected_item: root.Y_Plot_value = args[1]
    OutputField:
        height: dp(20)
        size_hint: 1, None
        id: output_regression
        label_text: 'output regression'
    OutputField:
        height: dp(20)
        size_hint: 1, None
        id: output_save_csv
        label_text: 'output save csv'
    OutputField:
        height: dp(20)
        size_hint: 1, None
        id: output_plot
        label_text: 'output plot'
